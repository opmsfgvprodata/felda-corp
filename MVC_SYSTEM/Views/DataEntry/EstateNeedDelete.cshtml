@model MVC_SYSTEM.ModelsCorporate.tbl_PerluLadang

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 class="modal-title">@GlobalResEstateNeed.lblTitleDelete</h3>
</div>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="modal-body">

        <div class="form-horizontal">
            <span id="progress" class="text-center" style="display: none;">
                <img src="~/Asset/Images/wait.gif" alt="wait" />
            </span>
            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResEstateNeed.lblWilayah</label>
                <div class="col-md-9">
                    @Html.TextBox("WilayahName", null, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResEstateNeed.lblLdg</label>
                <div class="col-md-9">
                    @Html.TextBox("LadangName", null, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResEstateNeed.lblYear</label>
                <div class="col-md-9">
                    @Html.EditorFor(model => model.fld_Tahun, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResEstateNeed.lblLuas</label>
                <div class="col-md-9">
                    @Html.EditorFor(model => model.fld_Luas, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResEstateNeed.lblPerlu</label>
                <div class="col-md-9">
                    @Html.EditorFor(model => model.fld_Perlu, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-9 col-sm-offset-3">
                    <h3>
                        <span class="label label-danger">
                            @GlobalResEstateNeed.DeleteMsg
                        </span>
                    </h3>
                </div>
            </div>
        </div>
    </div>


    <div class="modal-footer">
        <button class="btn btn-primary" data-dismiss="modal">@GlobalResEstateNeed.btnNo</button>
        <input class="btn btn-danger" type="submit" value=@GlobalResEstateNeed.btnYes />
    </div>
}