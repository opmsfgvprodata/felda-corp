                            <thead>
                                <tr>
                                    @*sarah modified *@
                                    @*<th colspan="28" style="text-align:center;vertical-align:middle;">*@
                                    <th colspan="29" style="text-align:center;vertical-align:middle;">


                                        @namapenuhsyarikat
                                        <br />
                                        (@namasyarikat)
                                        <br />
                                        @GlobalResCorp.hdrReportTitleLapKatPekTem
                                        <br />
                                        @ViewBag.Year
                                    </th>
                                </tr>
                                @if (ViewBag.IncLdg2 != 1)
                                {
                                    <tr>
                                        @*sarah modified*@
                                        @*<th colspan="28" bgcolor="#ffcc80" style="color:black;text-align:center;vertical-align:middle !important;border:1px solid black;" border="1">*@
                                        <th colspan="29" bgcolor="#ffcc80" style="color:black;text-align:center;vertical-align:middle !important;border:1px solid black;" border="1">
                                            @selectionheader
                                        </th>
                                    </tr>
                                }

                            <tr>
                                @if (ViewBag.Incldg == 0)
                                {
                                    <th rowspan="3" bgcolor="#ffcc80" style="color:black;text-align:center;vertical-align:middle !important;border:1px solid black;" border="1">@GlobalResCorp.lblRegion.ToUpper()</th>
                                }
                                else
                                {
                                    <th rowspan="3" bgcolor="#ffcc80" style="color:black;text-align:center;vertical-align:middle !important;border:1px solid black;" border="1">@GlobalResCorp.hdrLdg</th>
                                }
                                <!--<th rowspan="3" bgcolor="#ffcc80" style="color:black;text-align:center;vertical-align:middle !important;border:1px solid black;" border="1">WILAYAH</th>-->
