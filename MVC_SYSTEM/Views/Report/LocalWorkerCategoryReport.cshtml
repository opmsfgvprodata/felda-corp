                                        peneroka = peneroka + item.fld_Peneroka;
                                        penerokawati = penerokawati + item.fld_Penerokawati;
                                        belia = belia + item.fld_Belia;
                                        beliawanis = beliawanis + item.fld_Beliawanis;
                                        @*sampai sini*@
                                        jumlah = jumlah + item.fld_Jumlah;

                                    }
                                    RowNo++;
                                }
                            </tbody>
                            <tfoot>
                                <tr>
                                    @if (ViewBag.Incldg == 0)
                                    {
                                        <td colspan="1" bgcolor="#f4a142" style="color:black; text-align: center; vertical-align:middle !important;border:1px solid black;" border="1">@GlobalResActiveWorker.hdrTotal</td>
                                    }
                                    else
                                    {
                                        <td colspan="1" bgcolor="#f4a142" style="color:black; text-align: center; vertical-align:middle !important;border:1px solid black;" border="1">@GlobalResActiveWorker.hdrTotal</td>
                                    }
                                    @*<td bgcolor="#f4a142" style="color:black; text-align: center; vertical-align:middle !important;border:1px solid black;" border="1">@jumlah</td>*@
