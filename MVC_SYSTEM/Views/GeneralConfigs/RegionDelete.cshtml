@model MVC_SYSTEM.ModelsCorporate.tbl_Wilayah

@{
    Layout = null;
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 class="modal-title">@GlobalResRegion.lblTitleDelete</h3>
</div>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="modal-body">

        <div class="form-horizontal">
            <span id="progress" class="text-center" style="display: none;">
                <img src="~/Asset/Images/wait.gif" alt="wait" />
            </span>
            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResRegion.hdrRegion</label>
                <div class="col-md-9">
                    @Html.TextBox("fld_WlyhName", null, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-2">@GlobalResRegion.hdrEmel</label>
                <div class="col-md-9">
                    @Html.TextBox("fld_WlyhEmail", null, htmlAttributes: new { @class = "form-control", disabled = "disabled" })
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-9 col-sm-offset-3">
                    <h3>
                        <span class="label label-danger">
                            @GlobalResRegion.DeleteMsg
                        </span>
                    </h3>
                </div>
            </div>
        </div>
    </div>


    <div class="modal-footer">
        <button class="btn btn-primary" data-dismiss="modal">@GlobalResRegion.btnNo</button>
        <input class="btn btn-danger" type="submit" value=@GlobalResRegion.btnYes />
    </div>
}